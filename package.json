{
  "name": "@universalstandard/ook",
  "version": "1.2.0",
  "main": "bundle.js",
  "husky": {
    "hooks": {
      "pre-push": "run-s build test"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "run-s build jest",
    "jest": "jest"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-optional-chaining"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^0.8.2",
    "glamor": "^2.20.40",
    "known-css-properties": "^0.14.0",
    "lodash": "^4.17.11",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.16.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.1.0"
  },
  "license": "MIT"
}